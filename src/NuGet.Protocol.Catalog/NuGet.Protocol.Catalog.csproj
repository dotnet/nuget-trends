<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>NuGet.Protocol.Catalog</RootNamespace>
    <PackageId>NuGet.Protocol.Catalog</PackageId>
    <PackageVersion>0.2.0-beta</PackageVersion>
    <Description>A .NET library for consuming the NuGet API's catalog resource.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="7.0.1" />
    <PackageReference Include="NuGet.Protocol" Version="6.6.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="Sentry" Version="$(SentryVersion)" />
  </ItemGroup>

</Project>
