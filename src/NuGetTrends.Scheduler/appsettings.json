{
  "AllowedHosts": "*",
  "Hangfire": {
    "WorkerCount": 2
  },
  "DailyDownloadWorker": {
    "WorkerCount": 8
  },
  "NuGetResilience": {
    "TimeoutSeconds": 10,
    "TimeoutRetryMultiplier": 2.0,
    "MaxRetryAttempts": 2,
    "RetryDelaySeconds": 2
  },
  "Sentry": {
    "TracesSampleRate": 0.05,
    "EnableLogs": true
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Sentry",
        "Args": {
          "EnableLogs": true,
          "MinimumBreadcrumbLevel": "Debug"
        }
      }
    ]
  }
}
